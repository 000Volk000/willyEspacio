   (retract ?act)
   (assert (act algo))
    (retract ?pos)
   (assert (pos (+ ?x1 1) ?y1))
   (retract ?casilla)
   (assert (casilla (x ?x1) (y ?y1) (visitado true) (radiacion ?radiacion)))
)

(defrule actAntWest  (declare (salience 1000))
   ?act <- (act nada)
   ?ant <- (ant ?)
   (hice west)
   ?pos <- (pos ?x1 ?y1)
   ?casilla <- (casilla (x ?x1) (y ?y1) (visitado ?) (radiacion ?radiacion))
